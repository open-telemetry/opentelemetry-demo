{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--require ./utils/telemetry/Instrumentation.js' next dev",
    "build": "next build",
    "start": "node --require ./Instrumentation.js server.js",
    "lint": "next lint",
    "cy:open": "cypress open",
    "grpc:generate": "mkdir -p ./protos && protoc -I ./pb  --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./protos --ts_proto_opt=outputServices=grpc-js demo.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.0",
    "@grpc/grpc-js": "1.12.6",
    "@openfeature/flagd-provider": "0.13.3",
    "@openfeature/flagd-web-provider": "0.7.3",
    "@openfeature/react-sdk": "1.0.2",
    "@opentelemetry/api": "1.9.0",
    "@splunk/otel": "3.3.0",
    "@tanstack/react-query": "5.83.0",
    "cookies-next": "6.1.0",
    "currency-symbol-map": "5.1.0",
    "dotenv": "17.2.3",
    "dotenv-expand": "12.0.3",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "0.34.4",
    "styled-components": "6.1.19",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/styled-components": "5.1.35",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "cypress": "15.5.0",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "openapi-typescript": "7.10.1",
    "ts-proto": "2.8.3",
    "typescript": "5.9.3"
  }
}

    "@opentelemetry/auto-instrumentations-web": "0.47.0",
    "@opentelemetry/context-zone": "2.0.1",
    "@opentelemetry/core": "2.0.1",
    "@opentelemetry/exporter-metrics-otlp-grpc": "0.201.1",

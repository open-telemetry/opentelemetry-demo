{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run grpc:generate && NODE_OPTIONS='-r ./utils/telemetry/Instrumentation.js' next dev",
    "build": "next build",
    "start": "node --require ./Instrumentation.js server.js",
    "lint": "next lint",
    "cy:open": "cypress open",
    "grpc:generate": "mkdir -p ./protos && protoc -I ./pb  --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./protos --ts_proto_opt=outputServices=grpc-js demo.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.9.0",
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/auto-instrumentations-node": "0.39.1",
    "@opentelemetry/auto-instrumentations-web": "0.33.1",
    "@opentelemetry/context-zone": "1.15.2",
    "@opentelemetry/core": "1.15.2",
    "@opentelemetry/exporter-metrics-otlp-grpc": "0.41.2",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.41.2",
    "@opentelemetry/exporter-trace-otlp-http": "0.41.2",
    "@opentelemetry/instrumentation": "0.41.2",
    "@opentelemetry/resource-detector-alibaba-cloud": "0.28.1",
    "@opentelemetry/resource-detector-aws": "1.3.1",
    "@opentelemetry/resource-detector-container": "0.3.1",
    "@opentelemetry/resource-detector-gcp": "0.29.1",
    "@opentelemetry/resources": "1.15.2",
    "@opentelemetry/sdk-metrics": "1.15.2",
    "@opentelemetry/sdk-node": "0.41.2",
    "@opentelemetry/sdk-trace-base": "1.15.2",
    "@opentelemetry/sdk-trace-node": "1.15.2",
    "@opentelemetry/sdk-trace-web": "1.15.2",
    "@tanstack/react-query": "4.32.6",
    "cookies-next": "2.1.2",
    "currency-symbol-map": "5.1.0",
    "dotenv": "16.3.1",
    "dotenv-expand": "10.0.0",
    "next": "12.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.5",
    "styled-components": "6.0.7",
    "ts-proto": "1.156.6",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "9.0.2",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "cypress": "10.4.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.17",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "openapi-typescript": "6.5.0",
    "typescript": "5.1.6"
  }
}

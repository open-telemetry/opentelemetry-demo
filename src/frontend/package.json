{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run grpc:generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "grpc:generate": "mkdir -p ./protos && protoc -I ./pb  --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./protos --ts_proto_opt=outputServices=grpc-js demo.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@types/styled-components": "5.1.25",
    "currency-symbol-map": "^5.1.0",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.1",
    "styled-components": "5.3.5",
    "ts-proto": "^1.118.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "openapi-typescript": "^5.4.1",
    "typescript": "4.7.4"
  }
}

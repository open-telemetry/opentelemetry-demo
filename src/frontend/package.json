    "@opentelemetry/auto-instrumentations-node": "0.56.1",
    "@opentelemetry/auto-instrumentations-web": "0.45.1",
    "@opentelemetry/context-zone": "1.30.1",
    "@opentelemetry/core": "1.30.1",
    "@opentelemetry/exporter-metrics-otlp-grpc": "0.57.2",

{#- This template generates mkdocs.yml with dynamic navigation -#}
{#- It inherits from mkdocs-base.yml for all static configuration -#}
{#- Context: ctx.registries (list of {name}), ctx.services (list of {name}) -#}
# NOTE: THIS FILE IS AUTOGENERATED. DO NOT EDIT BY HAND.
# see templates/markdown/mkdocs_nav.yml.j2
# Inherits static configuration from mkdocs-base.yml

INHERIT: mkdocs-base.yml

nav:
  - Home: README.md
  - Attributes:
{%- for registry in ctx.registries %}
    - {{ registry.name | replace("_", " ") | title }}: attributes/{{ registry.id | split(".") | last | safe }}.md
{%- endfor %}
  - Services:
{%- for service in ctx.services %}
    - {{ service.name | replace("_", " ") | title }}: services/{{ service.name | safe}}.md
{%- endfor %}


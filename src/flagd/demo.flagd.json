{
  "$schema": "https://flagd.dev/schema/v0/flags.json",
  "flags": {
    "adFailure": {
      "defaultVariant": "off",
      "description": "Fail ad service",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    },
    "adHighCpu": {
      "defaultVariant": "off",
      "description": "Triggers high cpu load in the ad service",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    },
    "adManualGc": {
      "defaultVariant": "off",
      "description": "Triggers full manual garbage collections in the ad service",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    },
    "cartFailure": {
      "defaultVariant": "off",
      "description": "Fail cart service",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    },
    "imageSlowLoad": {
      "defaultVariant": "off",
      "description": "slow loading images in the frontend",
      "state": "ENABLED",
      "variants": {
        "10sec": 10000,
        "5sec": 5000,
        "off": 0,
        "on": 20000
      }
    },
    "kafkaQueueProblems": {
      "defaultVariant": "off",
      "description": "Overloads Kafka queue while simultaneously introducing a consumer side delay leading to a lag spike",
      "state": "ENABLED",
      "variants": {
        "off": 0,
        "on": 100
      }
    },
    "largeImage": {
      "defaultVariant": "off",
      "description": "replace image with very large image",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    },
    "loadgeneratorFloodHomepage": {
      "defaultVariant": "off",
      "description": "Flood the frontend with a large amount of requests.",
      "state": "ENABLED",
      "variants": {
        "off": 0,
        "on": 100
      }
    },
    "paymentFailure": {
      "defaultVariant": "off",
      "description": "Fail payment service charge requests n%",
      "state": "ENABLED",
      "variants": {
        "10%": 0.1,
        "100%": 1,
        "25%": 0.25,
        "50%": 0.5,
        "75%": 0.75,
        "90%": 0.95,
        "off": 0
      }
    },
    "paymentSupportedCardsProblem": {
      "defaultVariant": "on",
      "description": "Supported credit card types",
      "state": "ENABLED",
      "variants": {
        "off": {
          "american-express": "American Express",
          "mastercard": "Mastercard",
          "visa": "Visa"
        },
        "on": {
          "american_express": "American Express",
          "mastercard": "Mastercard",
          "visa": "Visa"
        }
      }
    },
    "paymentUnreachable": {
      "defaultVariant": "off",
      "description": "Payment service is unavailable",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    },
    "productCatalogFailure": {
      "defaultVariant": "off",
      "description": "Fail product catalog service on a specific product",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    },
    "recommendationCacheFailure": {
      "defaultVariant": "off",
      "description": "Fail recommendation service cache",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    }
  }
}
          ]},
          { "matcher": { "id": "byName", "options": "P50Latency" }, "properties": [{ "id": "unit", "value": "ms" }] },
          { "matcher": { "id": "byName", "options": "P95Latency" }, "properties": [{ "id": "unit", "value": "ms" }] },
          { "matcher": { "id": "byName", "options": "P99Latency" }, "properties": [{ "id": "unit", "value": "ms" }] },
          { "matcher": { "id": "byName", "options": "RequestCount" }, "properties": [{ "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } }] }
        ]
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "RequestCount" }]
      },
      "targets": [{
        "database": "otel_demo",
        "datasource": { "type": "grafana-azure-data-explorer-datasource", "uid": "adx-telemetry" },
        "querySource": "raw",
        "rawMode": true,
        "query": "OTelTraces\n| extend Timestamp = ingestion_time(), ServiceName = tostring(ResourceAttributes[\"service.name\"])\n| where Timestamp > ago(1h)\n| where SpanKind == 'SPAN_KIND_SERVER'\n| where isnotempty(ServiceName)\n| summarize \n    RequestCount = count(),\n    ErrorCount = countif(StatusCode == 'STATUS_CODE_ERROR'),\n    P50Latency = percentile(Duration / 1000000, 50),\n    P95Latency = percentile(Duration / 1000000, 95),\n    P99Latency = percentile(Duration / 1000000, 99)\n    by ServiceName\n| extend ErrorRate = toreal(ErrorCount) / toreal(RequestCount)\n| project ServiceName, RequestCount, ErrorCount, ErrorRate, P50Latency, P95Latency, P99Latency\n| order by RequestCount desc",
        "refId": "A",
        "resultFormat": "table",
        "queryType": "KQL"
      }],
      "title": "Service Performance Summary",
      "type": "table"
    },
